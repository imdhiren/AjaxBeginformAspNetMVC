@model IEnumerable<AjaxBeginformAspNetMVC2.Models.Countries>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CountriesName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CountriesName)
        </td>
    </tr>
}

</table>
